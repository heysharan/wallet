generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int @id @default(autoincrement())
  firstname String
  lastname  String
  email     String  @unique
  password  String
  age       Int
}

model Merchant {
  id         Int @id @default(autoincrement())
  name       String
  email      String @unique
  auth_type  AuthType
}

enum AuthType{
  Google
  Github
}